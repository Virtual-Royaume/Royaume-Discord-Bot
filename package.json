{
  "name": "royaume-discord-bot",
  "displayName": "Royaume-Discord-Bot",
  "version": "3.0.0",
  "main": "build/src/client.js",
  "scripts": {
    "dev": "npx nodemon --exec node -r @swc-node/register -r tsconfig-paths/register ./src/client.ts dev",
    "start": "node -r @swc-node/register -r tsconfig-paths/register ./src/client.ts",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "gen:gql": "graphql-codegen --config gql-codegen.ts",
    "gen:gql:watch": "graphql-codegen --watch --config gql-codegen.ts",
    "test:unit": "vitest --ui"
  },
  "dependencies": {
    "chart.js": "^3.9.1",
    "chartjs-node-canvas": "^4.1.6",
    "cron": "^2.3.0",
    "dayjs": "^1.11.7",
    "discord.js": "^14.11.0"
  },
  "devDependencies": {
    "@bluzzi/eslint-config": "^1.1.0",
    "@graphql-codegen/cli": "3.3.0",
    "@graphql-codegen/client-preset": "3.0.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@swc-node/register": "^1.6.4",
    "@swc/core": "^1.3.52",
    "@types/cron": "^2.0.1",
    "@types/node": "^18.15.11",
    "@vitest/ui": "^0.30.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.38.0",
    "graphql": "^16.6.0",
    "nodemon": "^2.0.22",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  },
  "eslintConfig": {
    "extends": "@bluzzi",
    "ignorePatterns": [
      "src/utils/request/graphql"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
