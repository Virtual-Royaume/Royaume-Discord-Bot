{
  "name": "royaume-discord-bot",
  "displayName": "Royaume-Discord-Bot",
  "version": "3.1.0",
  "scripts": {
    "dev": "npx nodemon --exec node -r @swc-node/register -r tsconfig-paths/register ./src/client.ts dev",
    "start": "node -r @swc-node/register -r tsconfig-paths/register ./src/client.ts",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "gql:gen": "graphql-codegen --config gql-codegen.ts",
    "gql:gen:watch": "graphql-codegen --watch --config gql-codegen.ts",
    "test:unit": "vitest --ui",
    "ts:check": "tsc --noEmit"
  },
  "dependencies": {
    "chart.js": "^3.9.1",
    "chartjs-node-canvas": "^4.1.6",
    "cron": "^2.3.1",
    "dayjs": "^1.11.9",
    "discord.js": "^14.11.0",
    "rustic-error": "^0.2.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@bluzzi/eslint-config": "^1.1.0",
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/client-preset": "4.0.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@swc-node/register": "^1.6.6",
    "@swc/core": "^1.3.69",
    "@types/cron": "^2.0.1",
    "@types/node": "^20.4.2",
    "@vitest/ui": "^0.33.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "graphql": "^16.7.1",
    "nodemon": "^3.0.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0"
  },
  "eslintConfig": {
    "extends": "@bluzzi",
    "ignorePatterns": [
      "src/utils/request/graphql/code-gen"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
