{
  "name": "royaume-discord-bot",
  "displayName": "Royaume-Discord-Bot",
  "version": "3.0.0",
  "main": "build/src/client.js",
  "scripts": {
    "dev": "npx nodemon --exec node -r @swc-node/register -r tsconfig-paths/register ./src/client.ts dev",
    "start": "node -r @swc-node/register -r tsconfig-paths/register ./src/client.ts",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "gqlgen": "graphql-codegen --config gql-codegen.ts",
    "gqlgen:watch": "graphql-codegen --watch --config gql-codegen.ts"
  },
  "dependencies": {
    "axios": "^1.2.3",
    "chart.js": "^3.9.1",
    "chartjs-node-canvas": "^4.1.6",
    "cron": "^2.3.0",
    "dayjs": "^1.11.7",
    "discord.js": "^14.8.0"
  },
  "devDependencies": {
    "@bluzzi/eslint-config": "^0.8.2",
    "@graphql-codegen/cli": "2.16.4",
    "@graphql-codegen/client-preset": "1.2.6",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@swc-node/register": "^1.5.4",
    "@swc/core": "^1.3.24",
    "@types/cron": "^2.0.1",
    "@types/node": "^18.11.18",
    "dotenv": "^16.0.3",
    "eslint": "^8.32.0",
    "graphql": "^16.6.0",
    "nodemon": "^2.0.20",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "eslintConfig": {
    "extends": "@bluzzi",
    "ignorePatterns": [
      "src/utils/request/graphql"
    ]
  }
}
