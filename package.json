{
  "version": "2.6.0",
  "main": "build/src/Client.js",
  "scripts": {
    "dev": "node --watch -r @swc-node/register -r tsconfig-paths/register ./src/Client.ts dev",
    "start": "node -r @swc-node/register -r tsconfig-paths/register ./src/Client.ts",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "gqlgen": "graphql-codegen --config gql-codegen.ts",
    "gqlgen:watch": "graphql-codegen --watch --config gql-codegen.ts"
  },
  "dependencies": {
    "chart.js": "^3.9.1",
    "chartjs-node-canvas": "^4.1.6",
    "dayjs": "^1.11.7",
    "discord.js": "^14.7.1"
  },
  "devDependencies": {
    "@bluzzi/eslint-config": "^0.8.2",
    "@graphql-codegen/cli": "2.16.4",
    "@graphql-codegen/client-preset": "1.2.6",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@swc-node/register": "^1.5.4",
    "@swc/core": "^1.3.24",
    "@types/node": "^18.11.18",
    "dotenv": "^16.0.3",
    "eslint": "^8.32.0",
    "graphql": "^16.6.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "eslintConfig": {
    "extends": "@bluzzi",
    "ignorePatterns": [
      "src/utils/request/graphql"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/*", 
        "cosmiconfig-toml-loader", 
        "ts-node"
      ]
    }
  }
}
